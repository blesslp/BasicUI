<?xml version="1.0" encoding="utf-8"?>
<top.blesslp.view.FitsSystemLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/qmui_config_color_white"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <top.blesslp.view.TitleBar
        android:id="@+id/titleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fitsSystemWindows="true"
        app:titleBar_title="呵呵哒"
        app:titlebar_backgroundColor="@color/qmui_config_color_red"
        app:titlebar_leftAsGoBack="true" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
            android:id="@+id/btnJump"
            android:layout_width="180dp"
            android:layout_height="320dp"
            android:text="跳转"
            android:textColor="@color/qmui_config_color_white"
            android:textSize="14dp"
            app:qmui_backgroundColor="@color/qmui_config_color_50_blue"
            app:qmui_radius="4dp" />

        <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
            android:id="@+id/btnJump2"
            android:layout_width="180dp"
            android:layout_height="320dp"
            android:layout_marginLeft="180dp"
            android:text="跳转"
            android:textColor="@color/qmui_config_color_white"
            android:textSize="14dp"
            app:qmui_backgroundColor="@color/qmui_config_color_50_blue"
            app:qmui_radius="4dp" />
    </FrameLayout>

</top.blesslp.view.FitsSystemLinearLayout>